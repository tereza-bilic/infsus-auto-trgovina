{
  "name": "at",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "ts-node-dev src/server.ts",
    "tailwind:watch": "npx tailwindcss -i ./src/public/stylesheets/input.css -o ./src/public/stylesheets/output.css --watch ",
    "build": "npx prisma generate --schema=./src/prisma/schema.prisma",
    "migrate": "npx prisma migrate dev --schema=./src/prisma/schema.prisma --name",
    "apply-migrations": "npx prisma migrate deploy --schema ./src/prisma/schema.prisma",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@prisma/client": "^5.14.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "method-override": "^3.0.0",
    "morgan": "~1.9.1",
    "pg": "^8.11.5"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/method-override": "^0.0.35",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.12",
    "@types/pg": "^8.11.6",
    "@types/supertest": "^6.0.2",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    }
  }
}
